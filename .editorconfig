# http://editorconfig.org
root = true

[*]
indent_style = tab
indent_size = 2
tab_width = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.md]
trim_trailing_whitespace = false

# configuration for javascript, react JSX, and its TypeScript variants
[*.{js,jsx,ts,tsx}]
quote_type = single