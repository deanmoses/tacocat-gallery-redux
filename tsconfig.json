{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "baseUrl": "src",
        "paths": {
            "actions/*": [
                "actions/*"
            ],
            "components/*": [
                "components/*"
            ]
        }
    },
    "include": [
        "./src/**/*"
    ]
}